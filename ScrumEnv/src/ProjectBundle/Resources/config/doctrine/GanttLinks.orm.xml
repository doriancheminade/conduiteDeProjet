<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ProjectBundle\Entity\GanttLinks" table="gantt_links">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="source" type="integer" column="source" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="target" type="integer" column="target" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="type" type="string" column="type" length="1" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
