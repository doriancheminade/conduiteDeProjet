<html>
    <head>
        {% block stylesheets %}
        <link href="{{ asset('bundles/Project/css/main.css') }}" rel="stylesheet"  type="text/css"/>
        <link href="{{ asset('css/twbs/bootstrap.css') }}" rel="stylesheet" />
        {% endblock %}

    <body>
        {% block body %}
        <div id="project">
            {{ include('ProjectBundle::navbar.html.twig') }}
            <div class="panel panel-default">
                <div class="panel-heading">
                    <h1 class="panel-title">
                        sprint tasks done
                        {#sprint.dateBegining|date('Y-m-d')#} : {#sprint.dateEnd|date('Y-m-d')#}
                    </h1>
                </div>
                <div class="panel-body">
<center>

<img src="data:image/png;base64, {{ EncodedImage }}" />	

</center>
            </div>
            </div>
            <a href="{{ path('Sprint_task_list', { 'owner': owner, 'project': project, 'sprintId': sprintId, 'kanban_view':'ok', 'pertt_view' :'ok' }) }}"><button class="btn btn-primary" type="submit" > back to task_list   </button></a>
        {% endblock %}

    </div>
    
    </body>
</html>